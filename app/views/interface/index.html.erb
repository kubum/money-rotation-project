<div id="wrap">
	<div id="header-region"></div>
	<div id="main-region" class="container"></div>
	<div id="push"></div>
</div>
<div id="footer-region"></div>

<script type="text/javascript">
  <% if signed_in? -%>
    currentUser = new Page.Entities.User(<%= current_user.to_json.html_safe %>);
  <% else -%>
    currentUser = null;
  <% end -%>
  
  Page.start({
    currentUser: currentUser
  });
</script>